<h4>Issues</h4>

<ul>
  {{#each model as |issue|}}
  <li>
    <div>
      <a target="_blank" href={{issue.html_url}}>
        {{issue.title}}
      </a>
    </div>
    <div>
      #{{issue.number}}
      opened on
      {{issue.created_at__date}}
      by
      <a target="_blank" href={{issue.user.html_url}}>{{issue.user.login}}</a>
    </div>
  </li>
  {{/each}}
</ul>

{{outlet}}
